#define MAX_CHECK 100 // Maximum number to check

// 3. Prototypes
int isPossible(int n);
int findMaxImpossible();

// 4. Main
int main() {
    // 4.1 Variable declarations
    int maxImpossible;

    // 4.2 Calls to functions
    maxImpossible = findMaxImpossible();
